{
  "name": "jfelder/oracledb",
  "description": "Oracle DB driver for Laravel",
  "keywords": ["oracle", "laravel", "laravel 10", "pdo_oci", "oci8"],
  "license": "MIT",
  "authors": [
      {
        "name": "Jimmy Felder",
        "email": "jimmyfelder@gmail.com"
      }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0",
    "illuminate/pagination": "^10.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.5.1",
    "phpunit/phpunit": "^10.0.7",
    "laravel/pint": "^1.13"
},
  "autoload": {
    "psr-4": {
      "Jfelder\\": "src/Jfelder"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Jfelder\\OracleDB\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
          "Jfelder\\OracleDB\\OracleDBServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
