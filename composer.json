{
    "name": "jfelder/oracledb",
    "description": "Oracle DB driver for Laravel",
    "keywords": [
        "oracle",
        "laravel",
        "laravel 11",
        "pdo_oci",
        "oci8"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Felder",
            "email": "jimmyfelder@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^12.0",
        "illuminate/pagination": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6.10",
        "phpunit/phpunit": "^11.5.3|^12.0.1"
    },
    "autoload": {
        "psr-4": {
            "Jfelder\\": "src/Jfelder"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfelder\\OracleDB\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jfelder\\OracleDB\\OracleDBServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
